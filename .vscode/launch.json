  "version": "0.2.0",
  "configurations": [
    {
      "name": "(Windows) Launch [x86 DLL]",
      "type": "cppvsdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/Debug/main.exe",
      "args": [
        "-e",
        "mspaint.exe",
        "--dll",
        "C:/Users/ASD/Project/cplusplus/projet/command-line-injector/test-dll/test-dll-x86.dll",
        "--watch",
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "console": "externalTerminal"
    },
    {
      "name": "(Windows) Launch [x64 DLL]",
      "type": "cppvsdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/Debug/main.exe",
      "args": [
        "-e",
        "mspaint.exe",
        "--dll",
        "C:/Users/ASD/Project/cplusplus/projet/command-line-injector/test-dll/test-dll-x64.dll",
        "--watch",
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "console": "externalTerminal"
    },
    {
      "name": "(Windows) Attach",
      "type": "cppvsdbg",
      "request": "attach",
      "processId": "${command:pickProcess}"
    },
  ]
}